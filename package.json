{
  "name": "mock-api",
  "version": "1.0.0",
  "description": "Mock/Test API for testing the development of GraphQuill",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && nodemon server/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "NODE_ENV=production webpack",
    "dev": "NODE_ENV=development node server/index.js & NODE_ENV=development webpack-dev-server --open",
    "resetPsql": "node database/psql/resetPSQL.js && node database/psql/setupTablesPSQL.js && node database/psql/seedCustomers.js",
    "resetMongo": "node database/mongo/resetMongoDB.js && node database/mongo/seedMongoDB.js",
    "resetDbs": "npm run resetPsql && npm run resetMongo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HolyTriumvirate/MockAPI.git"
  },
  "keywords": [],
  "author": "The Holy Triumvirate",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HolyTriumvirate/MockAPI/issues"
  },
  "homepage": "https://github.com/HolyTriumvirate/MockAPI#readme",
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "nodemon": "^1.19.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "faker": "^4.1.0",
    "graphql": "^14.5.8",
    "graphql-tools": "^4.0.6",
    "mongoose": "^5.7.7",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "style-loader": "^1.0.0"
  }
